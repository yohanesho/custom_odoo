<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_module_scheduler_run" model="ir.cron">
			<field name="name">Process Notification Overdue Receiving Item</field>
			<field eval="True" name="active"/>
			<field name="user_id" ref="base.user_root"/>
			<field name="interval_number">5</field>
			<field name="interval_type">minutes</field>
			<field name="numbercall">-1</field>
			<field name="nextcall" eval="(DateTime.now() + timedelta(minutes=5)).strftime('%Y-%m-%d %H:%M:%S')"/>
			<field name="doall" eval="False"/>
			<field name="model">purchase.order</field>
			<field name="function">_scheduler_notif_obsolete_order</field>
			<field name="priority">1</field>
			<field name="args">()</field>
		</record>
    </data>
</odoo>