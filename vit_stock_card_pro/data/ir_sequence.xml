<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>

		<record id="seq_stock_card" model="ir.sequence">
			<field name="name">Stock Card</field>
			<field name="code">vit.stock_card</field>
			<field name="prefix">SC/%(year)s/%(month)s/</field>
			<field name="padding">3</field>
			<field name="next_number_actual">1</field>
			<field name="number_increment">1</field>
			<field name="implementation">standard</field>
		</record>	

		<record id="seq_stock_summary" model="ir.sequence">
			<field name="name">Stock Summary</field>
			<field name="code">vit.stock_summary</field>
			<field name="prefix">SS/%(year)s/%(month)s/</field>
			<field name="padding">3</field>
			<field name="next_number_actual">1</field>
			<field name="number_increment">1</field>
			<field name="implementation">standard</field>
		</record>

	</data>
</openerp>